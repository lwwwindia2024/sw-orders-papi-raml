#%RAML 1.0 DataType
type: object
properties:
  orderId: string
  deliveryAddress: string
  totalPrice: integer
  orderStatus: string
  orderItems:
    type: array
    items: !include OrderItemDetails.raml
      
  customer:
    type: object
    properties:
      name: string
      email: string
      phone: string
  restaurant:
    type: object
    properties:
      restaurantName: string
      restaurantAddress: string
          
      